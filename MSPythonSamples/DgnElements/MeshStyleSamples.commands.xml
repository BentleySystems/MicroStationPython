<?xml version="1.0" encoding="utf-8" ?>
<KeyinTree xmlns="http://www.bentley.com/schemas/1.0/MicroStation/AddIn/KeyinTree.xsd">
    <RootKeyinTable ID="root">
        <Keyword SubtableRef="Mesh" CommandClass="MacroCommand" CommandWord="MESH">
            <Options Required="true"/>
        </Keyword>
    </RootKeyinTable>

    <SubKeyinTables>
        <KeyinTable ID="Mesh">
            <Keyword CommandWord="INDEXED FACE LOOPS"/>
            <Keyword CommandWord="QUAD GRID"/>
            <Keyword CommandWord="TRIANGLE GRID"/>
            <Keyword CommandWord="COORDINATETRIANGLE"/>
            <Keyword CommandWord="COORDINATEQUAD"/>
        </KeyinTable>
    </SubKeyinTables>

    <KeyinHandlers>
        <KeyinHandler Keyin="MESH INDEXED FACE LOOPS"                     Function="createIndexedFaceLoopMeshElement"/>
        <KeyinHandler Keyin="MESH QUAD GRID"                              Function="createQuadGridMesh"/>
        <KeyinHandler Keyin="MESH TRIANGLE GRID"                          Function="createTriangleGridMesh"/>
        <KeyinHandler Keyin="MESH COORDINATETRIANGLE"                     Function="createCoordinateTriangleMesh"/>
        <KeyinHandler Keyin="MESH COORDINATEQUAD"                         Function="createCoordinateQuadMesh"/>
    </KeyinHandlers>
</KeyinTree>
