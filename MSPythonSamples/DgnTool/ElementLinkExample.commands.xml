<?xml version="1.0" encoding="utf-8" ?>
<KeyinTree xmlns="http://www.bentley.com/schemas/1.0/MicroStation/AddIn/KeyinTree.xsd">
    <RootKeyinTable ID="root">
    <Keyword SubtableRef="ElementLink" CommandClass="MacroCommand" CommandWord="ELEMENTLINK">
            <Options Required="true" />
        </Keyword>
    </RootKeyinTable>

    <SubKeyinTables>
        <KeyinTable ID="ElementLink">
            <Keyword CommandWord="CREATELINK" />
            <Keyword CommandWord="DELETE" />
            <Keyword CommandWord="MODIFY" />
            <Keyword CommandWord="EXTRACT" />
        </KeyinTable>
    </SubKeyinTables>
    
    <KeyinHandlers>
        <KeyinHandler Keyin="ELEMENTLINK CREATELINK"                     Function="createElementLink" />
        <KeyinHandler Keyin="ELEMENTLINK DELETE"                        Function="deleteElementLink" />
        <KeyinHandler Keyin="ELEMENTLINK MODIFY"                        Function="modifyElementLink" />
        <KeyinHandler Keyin="ELEMENTLINK EXTRACT"                       Function="extractElementLink" />
    </KeyinHandlers>
</KeyinTree>